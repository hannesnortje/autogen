{
  "name": "autogen-mcp",
  "displayName": "AutoGen MCP",
  "description": "AutoGen Model Context Protocol integration for VS Code",
  "version": "0.1.0",
  "publisher": "hannesn",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hannesnortje/autogen"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autogen.startSession",
        "title": "Start AutoGen Session",
        "category": "AutoGen"
      },
      {
        "command": "autogen.stopSession",
        "title": "Stop AutoGen Session",
        "category": "AutoGen"
      },
      {
        "command": "autogen.searchMemory",
        "title": "Search Memory",
        "category": "AutoGen"
      },
      {
        "command": "autogen.addObjective",
        "title": "Add Objective",
        "category": "AutoGen"
      },
      {
        "command": "autogen.connect",
        "title": "Connect to Server",
        "category": "AutoGen"
      },
      {
        "command": "autogen.disconnect",
        "title": "Disconnect from Server",
        "category": "AutoGen"
      },
      {
        "command": "autogen.toggleConnection",
        "title": "Toggle Connection",
        "category": "AutoGen"
      },
      {
        "command": "autogen.configureWorkspace",
        "title": "Configure Workspace",
        "category": "AutoGen"
      },
      {
        "command": "autogen.showDashboard",
        "title": "Show Dashboard",
        "category": "AutoGen"
      },
      {
        "command": "autogen.showDashboard",
        "title": "Show Dashboard",
        "category": "AutoGen"
      },
      {
        "command": "autogen.checkServerStatus",
        "title": "Check Server Status",
        "category": "AutoGen"
      },
      {
        "command": "autogen.refreshSessions",
        "title": "Refresh Sessions",
        "category": "AutoGen"
      },
      {
        "command": "autogen.refreshMemory",
        "title": "Refresh Memory",
        "category": "AutoGen"
      },
      {
        "command": "autogen.openSessionDashboard",
        "title": "Open Session Dashboard",
        "category": "AutoGen"
      },
      {
        "command": "autogen.viewAgentDetails",
        "title": "View Agent Details",
        "category": "AutoGen"
      },
      {
        "command": "autogen.openMemoryExplorer",
        "title": "Open Memory Explorer",
        "category": "AutoGen"
      },
      {
        "command": "autogen.quickActions",
        "title": "Quick Actions",
        "category": "AutoGen"
      },
      {
        "command": "autogen.refreshAll",
        "title": "Refresh All Views",
        "category": "AutoGen"
      },
      {
        "command": "autogen.configureAgent",
        "title": "Configure Agent",
        "category": "AutoGen"
      },
      {
        "command": "autogen.startSessionWithParams",
        "title": "Start Session (Advanced)",
        "category": "AutoGen"
      },
      {
        "command": "autogen.searchMemoryAdvanced",
        "title": "Search Memory (Advanced)",
        "category": "AutoGen"
      },
      {
        "command": "autogen.addObjectiveAdvanced",
        "title": "Add Objective (Advanced)",
        "category": "AutoGen"
      },
      {
        "command": "autogen.stopSessionAdvanced",
        "title": "Stop Session (Advanced)",
        "category": "AutoGen"
      },
      {
        "command": "autogen.startServer",
        "title": "Start MCP Server",
        "category": "AutoGen"
      },
      {
        "command": "autogen.openMemoryAnalyticsDashboard",
        "title": "Open Memory Analytics Dashboard",
        "category": "AutoGen"
      },
      {
        "command": "autogen.openCrossProjectPanel",
        "title": "Open Cross-Project Intelligence",
        "category": "AutoGen"
      },
      {
        "command": "autogen.optimizeMemory",
        "title": "Optimize Memory",
        "category": "AutoGen"
      },
      {
        "command": "autogen.registerProject",
        "title": "Register Project",
        "category": "AutoGen"
      },
      {
        "command": "autogen.getProjectRecommendations",
        "title": "Get Project Recommendations",
        "category": "AutoGen"
      }
    ],
    "views": {
      "autogen": [
        {
          "id": "autogen.sessionView",
          "name": "Sessions",
          "when": "true",
          "icon": "./media/sessions.svg"
        },
        {
          "id": "autogen.memoryView",
          "name": "Memory",
          "when": "true",
          "icon": "./media/memory.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "autogen.refreshSessions",
          "when": "view == autogen.sessionView",
          "group": "navigation"
        },
        {
          "command": "autogen.refreshMemory",
          "when": "view == autogen.memoryView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "autogen.openSessionDashboard",
          "when": "view == autogen.sessionView && viewItem =~ /session-.*/",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "autogen",
          "title": "AutoGen",
          "icon": "./media/robot.svg"
        }
      ]
    },
    "configuration": {
      "title": "AutoGen MCP",
      "properties": {
        "autogen.server.url": {
          "type": "string",
          "default": "http://localhost:9000",
          "description": "AutoGen MCP server URL"
        },
        "autogen.server.autoStart": {
          "type": "boolean",
          "default": true,
          "description": "Automatically start server if not running"
        },
        "autogen.server.path": {
          "type": "string",
          "default": "/media/hannesn/storage/Code/autogen",
          "description": "Path to the AutoGen project directory (contains pyproject.toml)"
        },
        "autogen.server.healthCheckInterval": {
          "type": "number",
          "default": 30000,
          "description": "Health check interval in milliseconds"
        },
        "autogen.server.connectionTimeout": {
          "type": "number",
          "default": 10000,
          "description": "Connection timeout in milliseconds"
        },
        "autogen.memory.refreshInterval": {
          "type": "number",
          "default": 30,
          "description": "Memory refresh interval in seconds"
        },
        "autogen.websocketUrl": {
          "type": "string",
          "default": "ws://localhost:9001/ws",
          "description": "WebSocket URL for real-time updates"
        },
        "autogen.enableRealtime": {
          "type": "boolean",
          "default": true,
          "description": "Enable real-time data updates via WebSocket"
        },
        "autogen.agentConfigurations": {
          "type": "array",
          "default": [],
          "description": "Saved agent configurations",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "role": {
                "type": "string"
              },
              "systemMessage": {
                "type": "string"
              },
              "capabilities": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build:webview": "rollup -c",
    "watch:webview": "rollup -c -w",
    "package": "vsce package",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.5.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/vsce": "^3.6.0",
    "@eslint/js": "^9.9.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.9.0",
    "glob": "^11.0.3",
    "mocha": "^11.7.2",
    "sinon": "^21.0.0",
    "typescript": "^5.5.4",
    "rollup": "^4.21.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "tslib": "^2.6.3"
  },
  "dependencies": {
    "ws": "^8.18.0",
    "lit": "^3.2.0"
  }
}
