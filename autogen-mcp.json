{
  "version": "1.0",
  "mcp_server": {
    "endpoint": "http://localhost:9000",
    "websocket": true
  },
  "agents": {
    "enabled": true,
    "memory_collection": "autogen"
  },
  "workspace": {
    "auto_watch": true,
    "file_extensions": [
      ".py",
      ".ts",
      ".js",
      ".md",
      ".txt"
    ],
    "ignore_patterns": [
      "node_modules",
      "__pycache__",
      ".git"
    ]
  }
}
