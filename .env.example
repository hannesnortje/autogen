# Copy to .env and fill in values. Do NOT commit .env.

# --- LLM Credentials ---
# Gemini API key for cloud LLM usage
GEMINI_API_KEY=

# --- Qdrant Configuration ---
# Local Qdrant default url if using docker-compose
QDRANT_URL=http://localhost:6333
# Optional API key if your Qdrant instance uses auth
QDRANT_API_KEY=

# --- Retrieval Parameters ---
# Embedding model name and dimensions (to be finalized in Step 4)
EMBEDDING_MODEL=fastembed-default
EMBEDDING_DIM=384

# --- Agent Defaults ---
# Temperature settings per role (code/review should use 0)
AGENT_TEMPERATURE_CODE=0
AGENT_TEMPERATURE_REVIEW=0
AGENT_TEMPERATURE_GENERAL=0.2

# --- Observability ---
LOG_LEVEL=INFO
